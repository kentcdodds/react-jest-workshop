{
  "name": "react-jest-workshop",
  "private": true,
  "version": "1.0.0",
  "description": "ğŸ¯ A workshop repository for testing React âš› with Jest ğŸƒ",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest --coverage app/",
    "test:final": "jest --coverage app-final/",
    "watch:test": "npm run test -- --watch",
    "validate": "npm run lint && npm run test:final",
    "setup": "npm install && npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentcdodds/react-jest-workshop.git"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/react-jest-workshop/issues"
  },
  "homepage": "https://github.com/kentcdodds/react-jest-workshop#readme",
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "devDependencies": {
    "babel-jest": "15.0.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "enzyme": "2.4.1",
    "eslint": "3.5.0",
    "eslint-config-kentcdodds": "10.1.0",
    "jest": "15.1.1",
    "react-test-context-provider": "2.1.2",
    "react-test-renderer": "15.3.1"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache",
    "coverageThreshold": {
      "global": {
        "branches": 100,
